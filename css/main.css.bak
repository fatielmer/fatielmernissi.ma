* {
	margin: 0;
	padding: 0;
}
.active {background-color: #FFF;}
.activehome {
	background-color: #2C3E50;
	border-radius: 5px;
	border-radius: 5px;
	border-radius: 5px;
	color: #FFF;
}
.container {
	width: 1024px;
	margin: 0 auto;
}
#wrapper {
	width: 1366px;
	margin: 0 auto;
}

#wowslider-container1 {
	height: 39%;
	width: 101%;
}
p {
    color: rgba(8, 25, 27, 0.77);
        margin-left: -2px;

}

/* Header */

body{
	background-color: rgba(41, 41, 43, 0);
}
header {
	width: 100%;
	height: 90px;
	 background-color: #D02323;
    border-bottom: 3px solid #1D1A1A;
}

header .logo {
	float: left;
	margin-top: 24px;
}

header .logo h2 {
	font-size: 2.2em;
	color: #FFF;
}

header .logo h2 i {
	color: #FF7D82;
	margin-right: 7px;
	font-size: 1.2em;
}


/* Nav */

header nav ul {
	list-style: none;
	float: right;
}

header nav ul li {
	display: inline-block;
	margin: 37px 6px 0 0;
}

header nav ul li a {
	color: #E0FFF8;
	text-decoration: none;
	font-size: 14px;
	padding: 10px;
	-webkit-transition: background-color .5s;
	-moz-transition: background-color .5s;
	transition: background-color .5s;
}

header nav ul li a:hover {
	background-color: #2C3E50;
	border-radius: 5px;
	color: #FFF;
}


/* Section */

section {
	width: 101%;
	height: 190px;
	background: #D02323;
	color: #FFF;
	overflow: hidden;
	margin-top: -19px;
}

section .container div {
	margin-top: 30px;
	float: left;
}

section .container div .icon {
	width: 60px;
	height: 60px;
	background-color: rgba(0,0,0,.5);
	border-radius: 50px;
	text-align: center;
	line-height: 59px;
	font-size: 1.7em;
	text-shadow: 0 0 1px rgba(0,0,0,.5);
	-webkit-transition: border-radius .7s;
	-moz-transition: border-radius .7s;
	transition: border-radius .7s;
}

section .container div .icon:hover {border-radius: 10px;}

section .container div .description {
	width: 268px;
	text-align: left;
	margin-left:  12px;
}

section .container div .description h3 {font-size: 19px;

	line-height:64px;
}
section .container div .description p {font-size: 12px;}

/* Footer */

footer {
	width: 101%;
	height: 90px;
	background-color:#1D1A1A;
}

footer .container .copyright {
	float: left;
	margin-top: 44px;
	font-size: 14px;
	color: #EEE;
}

footer .container .copyright a {
	text-decoration: none;
	color: #EEE;
	-webbkit-transition: color .3s;
	-moz-transition: color .3s;
	transition: color .3s;
}

footer .container .copyright a:hover,
footer .container .downmenu ul li a:hover {
	color: #2B445D;
}

footer .container .downmenu {
	float: right;
}

footer .container .downmenu ul {
	list-style: none;
}

.hi{
	text-decoration: none;
	color: white;
}

footer .container .downmenu ul li {
	display: inline-block;
	margin: 42px 0 0 15px;
}

footer .container .downmenu ul li a {
	text-decoration: none;
	color: #FFF;
	font-size: 14px;
	-webbkit-transition: color .3s;
	-moz-transition: color .3s;
	transition: color .3s;
}